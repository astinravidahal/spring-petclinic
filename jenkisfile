pipeline   {
    agents any 
	stages {
	   stage('COMPILE STAGE'){
	    steps {
		
		sh 'mvn clean compile'
		
		}
		}
		stage('TEST STAGE'){
		   steps {
		
		   sh 'mvn test' 
		
		   }
		   }
		 stage ('DEPLOY STAGE'){
           steps {
		
             sh 'mvn deploy'
         
		 }
		 
	 
	    }
	 }
	}
