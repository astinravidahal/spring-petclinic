pipeline { 
agent any {
   stages {
    stage ('test'){
    stepsP{
      echo " this is just the test"
}
}
stage('compile'){
steps {
  echo "the code has compiled great" 
}}

stage ('run') { 
steps {
 echo "the code has run to the roof" 
}}

stage ('final'){
steps {
echo " looks like the code has run successfully"
}}
}
}
}

